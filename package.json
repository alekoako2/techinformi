{
  "name": "techinformi",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:en": "ng serve --configuration=en -- --port=4201",
    "start:ka": "ng serve --configuration=ka -- --port=4200",
    "start:both": "npm run start:ka npm run start:en",
    "build": "ng build --prod --i18n-locale ka --output-path=dist/ka --baseHref /ka/",
    "build:en": "ng build --configuration=en",
    "test": "ng test",
    "eslint": "eslint src/*.ts src/**/*.ts src/**/**/*.ts src/**/**/**/*.ts src/**/**/**/**/*.ts src/**/**/**/**/**/*.ts src/**/**/**/**/**/**/*.ts src/**/**/**/**/**/**/**/*.ts --fix",
    "stylelint": "npx stylelint src/styles.scss src/assets/styles/*.scss src/app/app.component.scss src/**/*.scss src/**/**/*.scss src/**/**/**/*.scss src/**/**/**/**/*.scss src/**/**/**/**/**/*.scss src/**/**/**/**/**/**/*.scss src/**/**/**/**/**/**/**/*.scss --fix",
    "lint": "yarn stylelint && yarn eslint",
    "e2e": "ng e2e",
    "translate": "ng xi18n --output-path src/locale",
    "build-locale:en": "ng build --prwod --i18n-locale en --i18n-format xlf --i18n-file src/locale/messages.en.xlf --output-path=dist/en --baseHref /en/",
    "build-locale": "npm run build && npm run build-locale:en",
    "codegen": "graphql-codegen --config codegen.yml && eslint src/app/modules/shared/models/generated/graphql.ts --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.1",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
    "@angular/core": "~9.1.1",
    "@angular/fire": "^6.0.0",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "~9.1.1",
    "@angular/localize": "~9.1.1",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/router": "~9.1.1",
    "@types/jest": "^25.2.1",
    "apollo-angular": "^1.9.0",
    "apollo-angular-link-http": "^1.10.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.14",
    "firebase": "^7.14.0",
    "firebase-admin": "^8.10.0",
    "graphql": "^14.5.0",
    "graphql-tag": "^2.10.0",
    "jquery": "^3.4.1",
    "ngx-slick-carousel": "^0.5.0",
    "rxjs": "~6.5.2",
    "slick-carousel": "^1.8.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~9.1.1",
    "@angular/language-service": "~9.1.1",
    "@angularclass/hmr": "^2.1.3",
    "@graphql-codegen/cli": "^1.13.2",
    "@graphql-codegen/fragment-matcher": "1.13.2",
    "@graphql-codegen/introspection": "1.13.2",
    "@graphql-codegen/typescript": "1.13.2",
    "@graphql-codegen/typescript-apollo-angular": "1.13.2",
    "@graphql-codegen/typescript-document-nodes": "1.13.2",
    "@graphql-codegen/typescript-graphql-files-modules": "1.13.2",
    "@graphql-codegen/typescript-operations": "1.13.2",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "codelyzer": "^5.2.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "prettier": "^2.0.4",
    "prettier-eslint": "^9.0.1",
    "protractor": "~5.4.2",
    "stylelint": "^13.3.2",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "ts-node": "~8.3.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.1",
    "typescript": "~3.8.3"
  }
}
