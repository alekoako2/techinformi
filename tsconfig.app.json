{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "strict": false,
    "outDir": "./out-tsc/app",
    "types": [],
    "baseUrl": "./",
    "paths": {
      "@graphql": [
        "src/app/shared/types/graphql/generated/graphql.types.ts"
      ],
      "@http/*": [
        "src/app/core/http/*"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@services/*": [
        "src/app/shared/services/*"
      ],
      "@env": [
        "src/environments/environment.ts"
      ],
      "@models/*": [
        "src/app/shared/models/*"
      ]
    }
  },
  "include": [
    "src/**/*.d.ts"
  ],
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ]
}
