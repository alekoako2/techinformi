{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "baseUrl": "./",
    "paths": {
      "@graphql": [
        "src/app/modules/shared/models/generated/graphql.ts"
      ],
      "@shared": [
        "src/app/modules/shared/shared.module.ts"
      ],
      "@services/*": [
        "src/app/modules/shared/services/*"
      ],
      "@env": [
        "src/environments/environment.ts"
      ],
      "@models/*": [
        "src/app/modules/shared/models/*"
      ]
    }
  },
  "include": [
    "src/**/*.d.ts"
  ],
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ]
}
