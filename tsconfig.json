{
  "compileOnSave": false,
  "compilerOptions": {
    "strict": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "declaration": false,
    "sourceMap": true,
    "outDir": "./dist/out-tsc",
    "module": "esnext",
    "target": "ES6",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "node",
      "jest"
    ],
    "lib": [
      "ES6",
      "dom",
      "esnext.asynciterable"
    ],
    "baseUrl": "./",
    "paths": {
      "@graphql": [
        "src/app/shared/types/graphql/generated/graphql.types.ts"
      ],
      "@http/*": [
        "src/app/core/http/*"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@services/*": [
        "src/app/shared/services/*"
      ],
      "@env": [
        "src/environments/environment.ts"
      ],
      "@models/*": [
        "src/app/shared/models/*"
      ]
    }
  }
}
